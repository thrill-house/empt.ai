{
  "name": "dash-game-vue-components",
  "description": "Vue.js components for the Dash Game",
  "version": "1.0.0",
  "author": "info@michaelpierce.trade",
  "url": "git@git.thrill.house:dash-game/vue-components.git",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "docs": "cross-env NODE_ENV=docs webpack --progress --hide-modules",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "pretty": "prettier '*.*' 'src/**/*.*' '!**/.*' '!*.html' --write",
    "stage-src": "git add src/*",
    "stage-dist": "git add dist/*"
  },
  "pre-commit": [
    "pretty",
    "build",
    "stage-src",
    "stage-dist"
  ],
  "dependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "lodash": "^4.17.11",
    "mathjs": "^4.4.2",
    "moment": "^2.22.2",
    "moment-duration-format": "^2.2.2",
    "npm": "^5.10.0",
    "numeral": "^2.0.6",
    "require-glob": "^3.2.0",
    "vue": "^2.5.17",
    "vue-localstorage": "^0.6.2",
    "vuex": "^3.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "gulp-postcss": "^7.0.1",
    "markdown": "^0.5.0",
    "node-sass": "^4.10.0",
    "only-if-changed-webpack-plugin": "0.0.1",
    "pre-commit": "^1.2.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.7",
    "svg-sprite-loader": "^4.1.3",
    "tailwindcss": "^0.7.2",
    "tailwindcss-layout": "^1.0.2",
    "tailwindcss-transforms": "^1.0.2",
    "tailwindcss-transitions": "^1.0.4",
    "vue-loader": "^13.7.3",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3"
  }
}
