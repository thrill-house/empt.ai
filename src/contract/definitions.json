{
  "Costs": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "data": {
        "type": "integer",
        "minimum": 1
      },
      "confidence": {
        "type": "integer",
        "minimum": 1
      }
    },
    "required": ["confidence", "data"],
    "description": "An object representing a cost."
  },
  "Base": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "type": {
        "type": "string",
        "minLength": 9,
        "maxLength": 9,
        "pattern": "^bandwidth$|^influence$"
      },
      "base": {
        "type": "integer"
      },
      "dependency": {
        "$ref": "#/definitions/Dependency"
      }
    },
    "required": ["base", "type"],
    "description": "An object representing a base."
  },
  "Factor": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "type": {
        "type": "string",
        "minLength": 7,
        "maxLength": 9,
        "pattern": "^bandwidth$|^influence$|^neutral$|^science$|^economy$|^society$"
      },
      "factor": {
        "type": "number",
        "multipleOf": 0.01
      },
      "dependency": {
        "$ref": "#/definitions/Dependency"
      }
    },
    "required": ["factor", "type"],
    "description": "An object representing a factor."
  },
  "Dependency": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "document": {
        "type": "string",
        "minLength": 5,
        "maxLength": 9,
        "pattern": "^slots$|^trainings$"
      },
      "where": {
        "type": "array",
        "minItems": 1,
        "items": {
          "type": "array",
          "additionalItems": false,
          "items": [
            {
              "type": "string",
              "minLength": 1,
              "maxLength": 128
            },
            {
              "type": "string",
              "enum": ["=="]
            },
            {
              "$ref": "#/definitions/Id"
            }
          ]
        }
      }
    },
    "required": ["document", "where"],
    "description": "An object representing a dependency."
  },
  "Feeling": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "emotionId": {
        "$ref": "#/definitions/Id"
      },
      "value": {
        "type": "integer",
        "minimum": 1,
        "maximum": 2
      }
    },
    "required": ["emotionId", "value"],
    "description": "An object representing a feeling."
  },
  "Bonus": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "type": {
        "type": "string",
        "minLength": 4,
        "maxLength": 10,
        "pattern": "^data$|^confidence$"
      },
      "bonus": {
        "type": "integer",
        "minimum": 1
      },
      "dependency": {
        "$ref": "#/definitions/Dependency"
      }
    },
    "required": ["bonus", "dependency", "type"],
    "description": "An object representing a bonus."
  },
  "Answer": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "description": {
        "$ref": "#/definitions/Description"
      },
      "value": {
        "type": "string",
        "minLength": 1
      }
    },
    "required": ["description", "value"],
    "description": "An object representing an answer."
  },
  "Meta": {
    "type": "array",
    "items": {
      "type": "array",
      "additionalItems": false,
      "items": [
        {
          "type": "string",
          "minLength": 1,
          "maxLength": 128
        },
        {
          "type": ["string", "number", "boolean"]
        }
      ]
    }
  },
  "Id": {
    "type": "string",
    "minLength": 42,
    "maxLength": 44,
    "pattern": "^[123456789ABCDEFGHJKLMNPQRSTUVWXYZabcdefghijkmnopqrstuvwxyz]+$",
    "description": "A string representing an ID."
  },
  "Title": {
    "type": "string",
    "minLength": 1,
    "maxLength": 128,
    "description": "A string representing a title."
  },
  "Description": {
    "type": "string",
    "minLength": 1,
    "maxLength": 1024,
    "description": "A string representing a description."
  }
}
